{
  "version": "1.0.0",
  "name": "trackx_server",
  "main": "src/main.ts",
  "author": "github.com/stardusteight-d4c",
  "license": "ISC",
  "scripts": {
    "dev": "ts-node src/main.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main",
    "start:tsnode": "npx ts-node src/main.ts",
    "start:server": "npx nodemon src/main.ts",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "npx ts-node src/config/prisma/seeds/main.ts",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push --accept-data-loss",
    "prisma:reset": "npx prisma db push --force-reset",
    "build": "nest build"
  },
  "prisma": {
    "schema": "src/config/prisma/schema.prisma"
  },
  "engines": {
    "node": "20.7.0",
    "npm": "10.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-fastify": "^10.3.0",
    "@prisma/client": "^5.8.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "jsonwebtoken": "^9.0.2"
  }
}
